{
    "rewrites": [
      { "source": "/", "destination": "/" },
      { "source": "/gallerypage", "destination": "/" },
      { "source": "/contact", "destination": "" },
      { "source": "/about", "destination": "/" },
      { "source": "/(.*)", "destination": "/" }
    ],
    "routes": [
      {
        "src": "^/static/(.*)$",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "dest": "/static/$1"
      }
    ]
  }
  