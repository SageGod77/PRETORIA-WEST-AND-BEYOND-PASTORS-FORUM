{
  "rewrites": [
    {
      "source": "/",
      "destination": "/"
    },
    {
      "source": "/gallerypage",
      "destination": "/gallerypage"
    },
    {
      "source": "/contact",
      "destination": "/contact"
    },
    {
      "source": "/about",
      "destination": "/about"
    },
    {
      "source": "/(.*)",
      "destination": "/404"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
